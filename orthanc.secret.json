{
  "Name": "orthanc-file-in-env-var",
  "RestApiWriteToFileSystemEnabled": true,
  "RemoteAccessAllowed" : true,
  "PostgreSQL" : {
    "EnableIndex": true,
    "EnableStorage": true,
    "Port": 5432,
    "Host": "postgres",
    "Database": "orthanc",
    "Username": "postgres",
    "Password": "postgres_pw",
    "EnableSsl": false,
    "Lock": false
  },
  "RegisteredUsers": {
    "demo": "demo"
  },
  "DicomWeb": {
    "Enable": true
  },
  "OrthancExplorer2": {
    "Enable": true,
    "IsDefaultOrthancUI": true,
    "UiOptions" : {
      "ViewersIcons" : {
        "osimis-web-viewer": "bi bi-eye",
        "stone-webviewer": "bi bi-eye-fill",
        "ohif": "bi bi-grid",
        "ohif-vr": "bi bi-grid-1x2",
        "ohif-tmtv": "bi bi-grid-3x3-gap",
        "ohif-seg": "fa-solid fa-shapes fa-button",
        "ohif-micro": "fa-solid fa-microscope fa-button",
        "meddream": "bi bi-columns-gap",
        "volview": "bi bi-box",
        "wsi": "fa-solid fa-microscope fa-button"
      },
      "DateFormat": "ddMMyyyy"
    }
  },
  "PythonScript" : "/etc/orthanc/hello.py",
  "LuaScripts" : [
    "/etc/orthanc/ai_trigger.lua"
  ]
}